{
  "name": "prismic-vuejs-starter",
  "version": "1.2.0",
  "description": "Vue.js starter project with content managed in Prismic",
  "license": "Apache-2.0",
  "repository": "github:prismicio/vuejs-starter",
  "author": "Prismic",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "prismic-dom": "^2.1.0",
    "prismic-vue": "^1.3.0",
    "vue": "^2.5.16",
    "vue-highlightjs": "^1.3.3",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@upstatement/eslint-config": "^0.4.2",
    "@upstatement/prettier-config": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "vue-eslint-parser": "^6.0.3",
    "vue-template-compiler": "^2.5.16"
  }
}
